.pageMainContentWrapper {
  height: 100%;
  width: 100%;
  top: 0%;
  position: fixed;
  /* The earlier format is clearer but can't be used with older browsers */
  /* padding-top: calc((0.85714286em * 1.14285714 * 2 + 1.5em) ~"+ calc(34.208px - 0.2233px)"); */
  padding-top: ~"calc(3.459183675em + 33.9847px)";
  overflow-y: auto;
}
.pageContainer {
  padding-bottom: 1em;
}
.ui.grid>.column.combatAndShipDisplay {
  /* padding-top: 6em; */
  /* padding-top: calc((0.92857143em * 1.14285714 * 2 + 1.6em) ~"+ 32.2344px"); */
  /* The earlier format is clearer but can't be used with older browsers */
  /* padding-top: calc((0.85714286em * 1.14285714 * 2 + 1.5em) ~"+ calc(34.208px - 0.2233px)"); */
  padding-top: ~"calc(3.459183675em + 33.9847px)";
  padding-left: 0em;
  max-height: 100%;
  bottom: 0px;
  top: 0px;
  /* Line required by old browsers and google's crawler. */
  left: 0px;
}
.ui.grid>.column.sidebarColumn {
  /* Max font size is 16 for header not 14 hence the 1.1428 */
  /* padding-top: calc((0.92857143em * 1.14285714 * 2 + 1.6em) ~"+ 32.2344px"); */
  /* padding-top: calc((0.857143em * 1.14285714 * 2 + 1.5em) ~"+ 34.2344px"); */
  /* The -0.223 is due to chrome being a special snowflake that rounds in very dumb ways. */
  /* The earlier format is clearer but can't be used with older browsers */
  /* padding-top: calc((0.85714286em * 1.14285714 * 2 + 1.5em) ~"+ calc(34.208px - 0.2233px)"); */
  padding-top: ~"calc(3.459183675em + 33.9847px)";
  background-color: var(--color3);
  max-height: 100%;
}
.sidebarToggleDiv {
  /* #padding-top: 6em; */
  margin-top: -6em;
  /* #background-color: rgb(70, 70, 70); */
  /* #background-color: rgb(180, 180, 180); */
  background-color: var(--color4);
  height: 100%;
