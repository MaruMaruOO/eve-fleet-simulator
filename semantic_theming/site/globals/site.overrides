/* Default values to pollyfill for browsers that don't support css vars. */
:root {
  --color2: rgba(37,44,51,1);
  --color9: rgba(17,19,21,1);
  --color6: rgba(24,29,32,1);
  --stock-font-color: rgba(143, 161, 178, 1);
  --high-contrast-font-color: rgba(169, 186, 203, 1);
  --inverse-color: rgb(255, 255, 255);
  --inverse-color1: rgba(255, 255, 255, 0.1);
  --inverse-color15: rgba(255, 255, 255, 0.15);
  --inverse-color25: rgba(255, 255, 255, 0.25);
  --inverse-color35: rgba(255, 255, 255, 0.35);
  --input-text-color: var(--stock-font-color);
  --input-text-color-focused: rgba(143,161,178,0.93);

  --semi-faded-stock-font-color: rgba(143, 161, 178, 0.75);
  --faded-stock-font-color: rgba(143, 161, 178, 0.6);
  --weak-stock-font-color: rgba(143, 161, 178, 0.15);

  --color3: var(--color6);
  --color4: rgba(0,0,0,1);
  --color5: rgba(52,60,69,1);
  --color6h: var(--color5);
  --color6a: var(--color2);
  --color10: var(--color6);
  /*--color11: var(--color9);*/
  --color11: var(--color2);
  --color7: var(--color6);
  --color7t: rgba(24,29,32,0.85);

  /*--button-opacity:0.82;*/
  --button-opacity:1;
  --semantic-invert-buttons:false;
  --semantic-button-color-one:pale-blue;
  --semantic-button-color-two:pale-blue;
  --semantic-button-color-three:pale-blue;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:grey-blue;
}

:root.defaultTheme.unused {
  --color0: rgba(0, 0, 0, 0);
  --color1: rgb(0, 0, 0);
  --color2: rgb(27, 28, 29); /* menu topbar */
  --color3: rgb(83, 87, 123);
  --color4: rgb(170, 170, 230);
  --color5: rgb(180, 180, 180);
  --color6: rgb(33, 37, 43);
  --color6h: rgb(49, 54, 63);
  --color7: rgb(70, 70, 70);
  --color7t: rgba(70,70,70,0.85);
  --color8: rgb(118, 118, 118);
  --color9: rgb(255, 255, 255);
  --color10: rgb(249, 250, 251);
  --color11: rgb(232, 232, 232);
  --stock-table-border-color: rgba(34, 36, 38, 0.15);
  --stock-table-cell-border-color: rgba(34, 36, 38, 0.1);
  --input-text-color: rgba(0,0,0,.87);
  --input-text-color-focused: rgba(0,0,0,.8);
  --semantic-button-color-one:teal;
  --semantic-button-color-two:teal;
  --semantic-button-color-three:teal;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:olive;
  --semantic-invert-buttons:false;
  --stock-font-color: rgba(0, 0, 0, 0.87);
  --high-contrast-font-color: rgba(0, 0, 0, 0.95);
  --inverse-color: rgb(0, 0, 0);
  --button-opacity:1;
}
:root.defaultTheme {
  --color2: rgba(232, 232, 231, 1);
  /*--color9: rgba(252, 252, 252, 1);*/
  --color9: rgba(249, 249, 250, 1);
