/* default firefox light theme */
:root.lightTheme {
  --color2: rgba(235, 236, 237, 1);
  --color9: rgba(252, 252, 252, 1);
  --color6: rgba(247, 247, 247, 1);
  --stock-font-color: rgba(24, 25, 26, 1);
  --high-contrast-font-color: rgba(41, 46, 51, 1);
  --inverse-color: rgb(0, 0, 0);
  --inverse-color1: rgba(0, 0, 0, 0.1);
  --inverse-color15: rgba(0, 0, 0, 0.15);
  --inverse-color25: rgba(0, 0, 0, 0.25);
  --inverse-color35: rgba(0, 0, 0, 0.35);
  --input-text-color: var(--stock-font-color);
  --input-text-color-focused: rgba(24,25,26,0.93);

  --semi-faded-stock-font-color: rgba(24, 25, 26, 0.45);
  --faded-stock-font-color: rgba(24, 25, 26, 0.25);
  --weak-stock-font-color: rgba(24, 25, 26, 0.15);

  --color3: var(--color6);
  --color4: rgba(170, 170, 170, 1);
  --color5: rgba(240, 241, 242, 1);
  --color6h: var(--color5);
  --color6a: var(--color2);
  --color10: var(--color6);
  --color11: var(--color2);
  --color7: var(--color6);
  --color7t: rgba(247,247,247,0.85);

  /*--button-opacity:0.9;*/
  --button-opacity:1;
  --semantic-invert-buttons:false;
  --semantic-button-color-one:mid-blue;
  --semantic-button-color-two:mid-blue;
  --semantic-button-color-three:mid-blue;
  --semantic-button-color-four:grey;
  --semantic-button-color-five-no-button-groups:bright-blue;
}
:root:not(.defaultThemeUnused) {
  --graph-font-color: var(--high-contrast-font-color);
}
:root.defaultThemeUnused {
  --graph-font-color: white;
}

/* Styling for scrollbars, mostly for chrome as semantic styles them by default. */
body>#root ::-webkit-scrollbar-track {
  background: var(--inverse-color1);
}
body>#root ::-webkit-scrollbar-thumb {
  background: rgba(0, 0, 0, 0.25);
  background: var(--inverse-color25);
}
body>#root ::-webkit-scrollbar-thumb:window-inactive {
  background: rgba(0, 0, 0, 0.15);
  background: var(--inverse-color15);
}
/* Stock light theme highlight looks nicer for blue tinted themes (including dark ones)*/
body>#root ::-webkit-scrollbar-thumb:hover {
  background: rgba(128, 135, 139, 0.8);
  /*background: var(--inverse-color35);*/
}


.ui.toggle.checkbox .box:before, .ui.toggle.checkbox label:before {
  background: rgba(128,128,128,.15);
}
.ui.checkbox input:not([type=radio]):indeterminate~.box:before,
.ui.checkbox input:not([type=radio]):indeterminate~label:before {
  background: rgba(255, 255, 255, 0.8);
}
.ui.checkbox input:not([type=radio])~.box:before,
.ui.checkbox input:not([type=radio])~label:before {
  box-shadow: 0 1px 1px 0 rgba(34,36,38,.15);
}
.ui.segment, .ui.inverted.segment,
.ui.primary.inverted.segment {
  background-color: var(--color9);
  color: var(--high-contrast-font-color);
}
.battleDisplayDimmer >.ui.dimmer {
  background-color: var(--color7t);
}
.ui.form .inline.field>label, .ui.form .field>label {
  color: var(--stock-font-color);
}
.ui.form textarea {
  color: var(--stock-font-color);
  background-color: var(--color9);
  border-color: var(--stock-table-border-color);
}
.ui.form textarea:focus {
  color: var(--high-contrast-font-color);
  background-color: var(--color9);
}
.ui.attached.header {
  background-color: var(--color9);
}
.ui.header {
  color: var(--stock-font-color);
}
.menu .ui.container {
  font-size: 1.14285714rem;
}
.ui.secondary.pointing.menu .item.header {
  margin: 0px 0px -2px;
}
.ui.secondary.inverted.menu {
  background: var(--color2);
}
.ui.secondary.inverted.pointing.menu {
  border-color: var(--weak-stock-font-color);
  border-width: 0px;
  border-bottom-width: 2px;
}
.ui.menu .item {
  line-height: 32.208px;
}
.ui.menu .square.icon {
  line-height: 1em;
}
.ui.active.button {
  opacity: 1;
}
.shipDisplaySettings .ui.dropdown .menu > .item {
  /* 2 rem more than normal to cover the icons */
  padding-right: 3.14286rem !important;
}
.ui.dropdown .menu {
  background: var(--color9);
}
.ui.dropdown .menu>.item {
  color: var(--stock-font-color);
}
:root:not(.defaultThemeUnused) .ui.dropdown .menu .selected.item,
:root:not(.defaultThemeUnused) .ui.dropdown.selected,
:root:not(.defaultThemeUnused) .ui.dropdown .menu>.item:hover,
:root:not(.defaultThemeUnused) .ui.dropdown .menu .active.item {
  color: var(--high-contrast-font-color);
}
:root:not(.defaultThemeUnused) .ui.menu .dropdown.item .menu {
  background: var(--color2);
  color: var(--stock-font-color);
}
:root:not(.defaultThemeUnused) .ui.menu .ui.dropdown .menu .active.item {
  color: var(--high-contrast-font-color) !important;
}
:root:not(.defaultThemeUnused) .ui.menu .ui.dropdown .menu>.item {
  color: var(--stock-font-color) !important;
}
.ui.menu .square.icon {
  background-color: currentColor;
  box-shadow: 0em 0em 0em 1px var(--stock-font-color);
}
/* these message colors are identical to the defaults on a white background */
.ui.error.message {
  background-color: rgba(255, 128, 128, 0.07);
  color: rgba(145, 65, 63, 1);
}
.ui.success.message {
  background-color: rgba(220, 255, 154, 0.1);
  color: rgba(46, 93, 47, 1);
}
.ui.success.message .header {
  color: rgba(46, 93, 47, 1);
}
.ui.error.message .header {
  color: rgba(145, 65, 63, 1);
}

.ui.button {
  opacity: var(--button-opacity);
}
.ui.buttons > .ui.button.split-left {
  width: 16.666%;
  border-right-color: var(--inverse-color15);
  border-right-width: 1px;
  border-right-style: solid;
}
.ui.buttons > .ui.button.split-right {
  width: 16.666%;
  border-left-color: var(--inverse-color15);
  border-left-width: 1px;
  border-left-style: solid;
}
.ui.header.sidebarHeader {
  color: var(--high-contrast-font-color);
}
.ui.label {
  color: var(--stock-font-color);
}
/* Centers and slightly increases the height of toggle checkboxes. */
/* Not for toggles directly inside buttons as they can use the buttons hitbox. */
:not(.button) > .ui.toggle.checkbox input.hidden + label {
  width: 3.5em;
  height: 2em;
  padding: 0px;
}
:not(.button) > .ui.toggle.checkbox input.hidden + label:before {
  margin-top: 0.25em;
  margin-bottom: 0.25em;
}
:not(.button) > .ui.toggle.checkbox input.hidden + label:after {
  margin-top: 0.25em;
  margin-bottom: 0.25em;
}

.ui.input > input::-webkit-input-placeholder {
  color: var(--faded-stock-font-color);
}
.ui.input > input::-moz-placeholder {
  color: var(--faded-stock-font-color);
}
.ui.input > input::placeholder {
  color: var(--faded-stock-font-color);
}
.ui.input.focus > input::-webkit-input-placeholder, .ui.input > input:focus::-webkit-input-placeholder {
  color: var(--semi-faded-stock-font-color);
}
.ui.input.focus > input::-moz-placeholder, .ui.input > input:focus::-moz-placeholder,{
  color: var(--semi-faded-stock-font-color);
}
.ui.input.focus > input::placeholder, .ui.input > input:focus::placeholder {
  color: var(--semi-faded-stock-font-color);
}

.ui.menu .item > .ui.input input {
  background-color: var(--color9);
}
.ui.menu .ui.dropdown .menu > .item.semi-attached-dropdown-item {
  padding: 0rem 1.14285714em !important;
  padding-bottom: 0.28571429em !important;
  text-align: right;
}
.item.semi-attached-dropdown-item > .checkbox {
  vertical-align: middle;
  padding-left: 0.64286em;
}
.item.semi-attached-dropdown-item > span {
  vertical-align: text-bottom;
}
.ui.menu .item.semi-attached-dropdown-item > .ui.input {
  margin: 0.4em 0em;
}
.item.semi-attached-dropdown-item > .checkbox > input.hidden {
  display: none;
}

.ui.dropdown .menu .item .dropdown.icon:before {
  content: "\F0D7";
}
.ui.menu .menu > .item > .ui.floating.dropdown > .menu {
  margin-top: 1.28571429em !important;
}
/*.ui.label.corner {
  border-color: var(--color11);
}*/
:not(.image) > .ui.label.corner {
  /* this allows the label to adjust to the theme automatically while remaining functionally opaque
     despite the transparency in --weak-stock-font-color */
  background: linear-gradient(225deg, var(--color9) calc(50% - 1px), transparent calc(50% - 1px));
  border-color: var(--weak-stock-font-color);
}
:not(.image) > .ui.label.corner:hover {
    /*background-color: #E0E0E0; 224 actual shadow is 212, 212, 213
    border-color: #E0E0E0;
    background-image: none;
    color: rgba(0, 0, 0, 0.8);*/
  border-color: rgb(224, 224, 224);
}
body {
  color: var(--stock-font-color);
  background: var(--color9);
}
.ui.progress>.label {
