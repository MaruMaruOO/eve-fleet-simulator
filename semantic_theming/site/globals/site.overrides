/* dnd table styling */
td >.ui.fluid.image >.ui.corner.label {
  height: ~"calc(2.8em - 1px)";
  width: ~"calc(2.8em - 1px)";
  border-color: inherit;
  top: 1px;
  right: 1px;
}
td > .ui.fluid.image > .ui.corner.label:hover {
  color: var(--high-contrast-font-color);
}
td >.ui.fluid.image >img {
  border-width: 1px;
  border-color: inherit;
  border-style: solid;
}
td >.ui.fluid.image >.ui.corner.label .icon {
  /* Reformatted for compatibility with older browsers. */
  /* --size-multi: calc(2.8 / 4); (0.7) */
  /* top: ~"calc(calc(0.64285714em * var(--size-multi)) * var(--size-multi))"; */
  /* left: ~"calc(0.78571429em * var(--size-multi))"; */
  top: 0.64285714em * 0.49;
  left: 0.78571429em * 0.7;
}
td >.ui.fluid.image >.ui.corner.label:after {
  border-bottom-width: ~"calc(2.8em - 1px)";
  border-right-width: ~"calc(2.8em - 1px)";
}
td >.shipStatBarDiv >.shipSimBar >.bar {
  background-color: var(--inherited-bg);
  min-width: 10%;
}

td>.shipStatBarDiv>.ui.progress>.label {
  margin: 0em;
  /* -1px to compensate for the border and keep consistent with other progress labels */
  margin-top: ~"calc(0.2em - 1px)";
  padding: 0px;
  background: transparent;
}
td>.shipStatBarDiv>.ui.progress>.label input {
  background-color: var(--color9);
  text-align: center;
  padding-right: 0px;
  padding-bottom: 0px;
  padding-top: 0px;
  line-height: 1.4285rem;
  font-weight: 700;
  width: 100%;
  padding: 0px;
  border-color: transparent;
  /* border-color: var(--inverse-color1); */
}
td>.shipStatBarDiv>.ui.progress>.label input:focus {
  border-color: rgb(133, 183, 217);;
}
td>.shipStatBarDiv>.ui.progress>.label input::-webkit-inner-spin-button {
  display: none;
}
td>.shipStatBarDiv>.ui.progress>.label input:enabled:read-write:hover::-webkit-inner-spin-button,
td>.shipStatBarDiv>.ui.progress>.label input:enabled:read-write:focus::-webkit-inner-spin-button {
  display: inline-block;
}
td>.shipStatBarDiv>.ui.progress>.label input[type=number] {
  -moz-appearance: textfield;
}
td>.shipStatBarDiv>.ui.progress>.label input[type=number]:enabled:hover,
td>.shipStatBarDiv>.ui.progress>.label input[type=number]:enabled:focus {
  -moz-appearance: number-input;
  overflow: hidden;
}

input[type=number] {
